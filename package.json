{
  "name": "netlify-fauna",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "faunadb": "^2.5.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "docs": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "checkForFaunaKey": "node ./scripts/check-for-fauna-key.js",
    "start": "npm-run-all --parallel checkForFaunaKey start:app start:server",
    "start:app": "react-scripts start",
    "start:server": "netlify-lambda serve functions -c ./webpack.config.js",
    "prebuild": "echo 'setup faunaDB' && npm run bootstrap",
    "build:origin": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "test": "react-scripts test --env=jsdom",
    "lambda": "netlify-lambda serve functions -c ./webpack.config.js",
    "lambda:build": "netlify-lambda build functions -c ./webpack.config.js",
    "rollup": "rollup -c -w",
    "rollup:build": "rollup -c",
    "live": "npx live-server --open=/public/ --proxy=/public/.netlify/functions/:http://localhost:9000/.netlify/functions/",
    "dev": "run-p checkForFaunaKey live lambda rollup",
    "build": "run-s lambda:build rollup:build"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.1",
    "markdown-magic": "^0.1.25",
    "netlify-lambda": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "live-server": "^1.2.1",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}